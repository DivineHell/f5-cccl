	},
	{
	  "name": "MyVaddr",
	  "autoDelete": "false",
	  "enabled": "no",
	  "address": "192.168.0.2"
	}
  ],
  "virtualServers": [{
	"name": "vs1",
	"destination": "/test/MyVaddr:80",
	"pool": "/test/pool1",
	"sourceAddress": "0.0.0.0/0",
	"enabled": true,
	"vlansEnabled": true,
	"vlans": ["/test/vlan-100", "/Common/http-tunnel"],
